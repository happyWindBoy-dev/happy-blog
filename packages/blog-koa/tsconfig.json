{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es6"],
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "sourceRoot": "src",
    "outDir": "dist",
    "emitDecoratorMetadata": true,
  },
  "exclude": ["dist", "node_modules"]
}
